{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "$GEMINI_API_KEY",
    "JWT_SECRET_KEY": "$JWT_SECRET_KEY",
    "DATABASE_URL": "$DATABASE_URL",
    "CORS_ORIGINS": "$CORS_ORIGINS"
  }
}
